{
  "name": "@6missedcalls/notion-cli",
  "version": "0.1.4",
  "description": "Fast Notion CLI for pages, databases, and blocks",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "notion": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
