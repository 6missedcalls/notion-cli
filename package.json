    "test": "vitest run",
    "prepublishOnly": "pnpm run build && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/6missedcalls/notion-cli.git"
  },
  "keywords": [
    "notion",
    "cli",
    "api",
    "markdown",
    "blocks"
  ],
  "author": "6missedcalls",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/6missedcalls/notion-cli/issues"
  },
  "homepage": "https://github.com/6missedcalls/notion-cli#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=22"
  }
